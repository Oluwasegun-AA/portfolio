{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --colors --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}' --ext .ts,.tsx",
    "fix": "run-s fix:*",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --ext .tsx,.ts --fix",
    "fix:prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "fix:eslint": "eslint 'src/**/*.{js,ts,tsx}' --ext .js,.ts --fix"
  },
  "dependencies": {
    "@apollo/client": "3.8.6",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "apollo": "2.34.0",
    "axios": "1.5.1",
    "dotenv": "16.3.1",
    "graphql": "16.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "typescript": "5.2.2",
    "web-vitals": "3.5.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@types/jest": "29.5.6",
    "@types/node": "20.8.7",
    "@types/react": "18.2.30",
    "@types/react-dom": "18.2.14",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "node-sass": "9.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
