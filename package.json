{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "dependencies": {
    "@apollo/client": "3.3.12",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react-dom": "^17.0.0",
    "apollo": "2.32.5",
    "axios": "0.21.1",
    "dotenv": "8.2.0",
    "graphql": "15.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --colors --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}' --ext .ts,.tsx",
    "fix": "run-s fix:*",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --ext .tsx,.ts --fix",
    "fix:prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "fix:eslint": "eslint 'src/**/*.{js,ts,tsx}' --ext .js,.ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "eslint": "7.22.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "node-sass": "^7.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1"
  }
}
